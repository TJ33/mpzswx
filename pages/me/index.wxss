.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.bt_box_view {
  margin-top: 50rpx;
  display: flex;
  flex: 1;
  justify-content: center;
}
.bt_box_view .bt_box {
  display: flex;
  width: 560rpx;
  height: 100rpx;
  background: #469CFF;
  border-radius: 10rpx;
  color: #fff;
  font-size: 32rpx;
  justify-content: center;
  align-items: center;
  box-shadow: 0px 6px 15px 0px rgba(47, 118, 188, 0.3);
}
.bt_box_view .bt_box_no {
  background: #919599;
  box-shadow: 0px 6px 15px 0px rgba(47, 118, 188, 0.3);
}
page {
  background: #fff;
}
.all {
  border-top: 1rpx #EBEBEB solid;
  background: #FFFFFF;
}
.all .top {
  display: flex;
  flex-direction: column;
}
.all .top .myhead {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 70rpx 100rpx 50rpx;
}
.all .top .myhead .namePhone {
  display: flex;
  flex-direction: column;
}
.all .top .myhead .namePhone .name {
  font-size: 55rpx;
  font-weight: bold;
  color: #303233;
}
.all .top .myhead .namePhone .phone {
  font-size: 26rpx;
  color: #303233;
}
.all .top .myhead .sq {
  font-size: 24rpx;
  background: #FFFFFF;
  border-radius: 4rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0px 2px 10px 0px rgba(69, 107, 144, 0.2);
  border: 1rpx #fff solid;
}
.all .top .myhead .sq .iconfont {
  font-size: 24rpx;
}
.all .top .myhead .icon-lujing {
  font-size: 140rpx;
}
.all .top .myhead .head {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}
.all .other {
  border-top: 1rpx #DAE0E5 solid;
  margin: 0 50rpx;
}
.all .other .choice {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom: 1rpx #DAE0E5 solid;
  padding: 50rpx 0;
}
.all .other .choice .textStyle {
  font-size: 32rpx;
  color: #000000;
  font-weight: 500;
}
.all .other .choice .icon-jiantou {
  font-size: 32rpx;
  color: #C2C7CC;
}
.Dialog_Box {
  width: 100%;
  height: 0;
  position: fixed;
  left: 0;
  top: 0;
  overflow: hidden;
}
.Dialog_Box_active {
  z-index: 4;
}
.Dialog_Box_hidden {
  z-index: -1;
}
.Dialog_Box_float {
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.55);
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 0;
  transition: all 0.5s;
}
.Dialog_Box .Dialog_ {
  width: 85%;
  background: #fff;
  border-radius: 4px;
  transform: scale(0.8);
  transition: all 0.3s;
  overflow: hidden;
}
.Dialog_title {
  font-size: 16px;
  line-height: 52px;
  font-weight: 600;
  text-align: center;
}
.Dialog_Btn {
  display: flex;
  align-items: center;
  border-top: 1px solid #f2f2f2;
  margin-top: 20px;
}
.Dialog_Btn > .Dialog_Btn_s {
  border: 0;
  border-radius: 0;
  font-size: 28rpx;
  color: #333;
  line-height: 50px;
  text-align: center;
  flex: 1;
}
.Dialog_Btn > .Dialog_Btn_hover {
  background: #f2f2f2;
}
.Dialog_Btn > .Dialog_Btn_s:last-child {
  border-left: 1px solid #f2f2f2;
  color: #409EFF;
}
.Dialog_content {
  padding: 20px;
  box-sizing: border-box;
  text-align: center;
}
.Dialog_Box_show {
  height: 100vh;
}
.Dialog_Box_float_show {
  opacity: 1;
}
.Dialog_Box .Dialog_show {
  transform: scale(1);
}
